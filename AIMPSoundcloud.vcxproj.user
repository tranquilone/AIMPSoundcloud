    <LocalDebuggerAttach>false</LocalDebuggerAttach>
    <LocalDebuggerWorkingDirectory>D:\AIMP4\</LocalDebuggerWorkingDirectory>
  </PropertyGroup>
  <PropertyGroup>
    <ShowAllFiles>false</ShowAllFiles>
  </PropertyGroup>
